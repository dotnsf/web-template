<%- include('./header', {}) %>

<link href="/css/<%= _name %>.css" rel="stylesheet"/>
<script src="/js/<%= _name %>.js"></script>

<title><%= _appname + " - " + _title %></title>
</head>
<body>

<%- include('./navi', { _appname: _appname, _title: _title }) %>

<div style="padding-top: 100px;">
  <bx-data-table>
    <bx-table>
      <bx-table-head>
      </bx-table-head>
      <bx-table-body>
        <bx-table-row>
          <bx-table-cell>#</bx-table-cell>
          <bx-table-cell><%= _id %></bx-table-cell>
        </bx-table-row>
        <bx-table-row>
          <bx-table-cell>name</bx-table-cell>
          <bx-table-cell><%= _record.name %></bx-table-cell>
        </bx-table-row>
        <bx-table-row>
          <bx-table-cell>description</bx-table-cell>
          <bx-table-cell><%= _record.description %></bx-table-cell>
        </bx-table-row>
      </bx-table-body>
    </bx-table>
  </bx-data-table>
</div>

<%- include('./links', { _links: _links }) %>

<%- include('./footer', {}) %>
