<%- include('./header', {}) %>

<link href="/css/<%= _name %>.css" rel="stylesheet"/>
<script src="/js/<%= _name %>list.js"></script>

<title><%= _appname + " - " + _title %></title>
</head>
<body>

<%- include('./navi', { _appname: _appname, _title: _title }) %>

<div class="container" style="padding-top: 100px;">
  <table class="table table-bordered">
    <thead>
      <tr><th>#</th><th>name</th></tr>
    </thead>
    <tbody>
<% for( var i = 0; i < _records.length; i ++ ){ %>
      <tr>
        <td><a href="/<%= _name.substr( 0, _name.length - 1 ) %>/<%= _records[i].id %>"><%= _records[i].id %></a></td>
        <td><a href="/<%= _name.substr( 0, _name.length - 1 ) %>/<%= _records[i].id %>"><%= _records[i].name %></a></td>
      </tr>
<% } %>
    </tbody>
  </table>
</div>

<%- include('./footer', {}) %>
